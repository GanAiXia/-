.container {
    padding-bottom: 100rpx;
}

.section {
    background-color: #fff;
}

.mlt30 {
    margin-left: 30rpx;
    margin-top: 10rpx;
}

.bord1 {
    border-bottom: 1px solid #f1f1f1;
}

.th1 .newshop {
    height: 70rpx;
    line-height: 70rpx;
}

.th1 .newshop text {
    margin-right: 10rpx;
}

.th1 .newshop .lptitle {
    font-weight: bold;
    color: #333;
}

.th1 .bd {
    border-bottom: 1px solid #f1f1f1;
    height: 170rpx;
    margin-right: 30rpx;
}

.th1 .hxprice {
    height: 60rpx;
}

.th1 .hxprice .num {
    color: #ff0000;
    font-size: 30rpx;
    font-weight: 400;
}

.th1 .hxprice .time {
    float: right;
    color: #999;
    margin-top: 15rpx;
}

.fcgray {
    color: #999;
}

.th1 .hxd1 {
    display: block;
    margin-bottom: 15rpx;
}

.th1 .hxparam {
    padding: 30rpx 0rpx;
}

.th1 .hxd1 view {
    display: inline-block;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.th1 .lpitem {
    font-size: 30rpx;
    line-height: 60rpx;
    height: 60rpx;
}

.th1 .lpitem .more {
    float: right;
    margin-right: 30rpx;
    color: #666;
    font-size: 35rpx;
}

.th1 .lpitem .txt {
    float: left;
}

.th1 .lpitem .link {
    color: #67A4D9;
    width: 500rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    display: block;
}

.yangbanjian {
    margin-top: 10rpx;
}

.barh3 {
    margin-left: 30rpx;
    height: 110rpx;
    line-height: 110rpx;
}

.barh3 .title {
    font-weight: bold;
    font-size: 37rpx;
}

.yangbanjian .real {
    padding-bottom: 30rpx;
    white-space: nowrap;
    display: flex;
    background-color: #ffffff;
    padding-right: 30rpx;
}

.real .fiximg {
    width: 400rpx;
    height: 300rpx;
    margin-left: 30rpx;
    display: inline-block;
    border: #f1f1f1 1px solid;
}

.real .fiximg image {
    width: 400rpx;
    height: 300rpx;
}

.loudong {
    margin-top: 10rpx;
    padding-bottom: 30rpx;
}

.loudong .txt {
    margin: 0px 30rpx;
    font-size: 30rpx;
    color: #666;
}

.space {
    margin-top: 10rpx;
    padding-left: 30rpx;
    padding-right: 30rpx;
}

.spacetab {
    padding-top: 30rpx;
    padding-bottom: 30rpx;
    display: block;
}

.spacetab .wordbox {
    display: inline-block;
    margin-right: 20rpx;
    vertical-align: middle;
}

.spacetab .wordbox text {
    font-size: 26rpx;
    height: 70rpx;
    line-height: 70rpx;
    padding: 10rpx 20rpx;
    background-color: #E6F7ED;
    border-radius: 5rpx;
    color: #00AE67;
}

.linebar {
    border-top: #f1f1f1 1px solid;
    padding-top: 30rpx;
    padding-bottom: 30rpx;
}

.linebar text {
    font-weight: 400;
}

.spacedesc .item {
    border-left: #f1f1f1 1px solid;
    position: relative;
    padding-left: 30rpx;
    margin-left: 10rpx;
}

.spacedesc .item:last-child {
    border-left: 0px;
}

.spacedesc .dot {
    width: 25rpx;
    height: 25rpx;
    border-radius: 50%;
    background: #dadada;
    vertical-align: middle;
    text-align: center;
    border: 5rpx solid #fff;
    position: absolute;
    left: -15rpx;
    top: -2rpx;
}

.spacedesc .item .desc .v1 {
    line-height: 28rpx;
}

.spacedesc .item .desc .v2 {
    line-height: 32rpx;
    margin-top: 0rpx;
    color: #777;
}

.spacedesc .item {
    padding-bottom: 50rpx;
}

.blue {
    color: #00AE67;
    font-weight: bold;
}

.spacehtml {
    line-height: 40rpx;
    padding-bottom: 50rpx;
    padding-left: 40rpx;
}

.copy {
    background-color: #eee;
    padding: 30rpx 60rpx;
    position: relative;
}

.copy .info {
    color: #666666;
    margin-left: 20rpx;
}

.copy text {
    display: table-cell;
}

.copy .copyicon {
    position: absolute;
    left: 30rpx;
    top: 25rpx;
    display: inline-block;
}

.copy .info {
    line-height: 30rpx;
}

.copy .info .ft22 {
    font-size: 22rpx;
    line-height: 30rpx;
    text-justify: inter-word;
    text-align: justify;
}

.bbmenu {
    position: fixed;
    bottom: 0px;
    left: 0px;
    width: 100%;
    border-top: #dadada 1px solid;
    background: #fff;
    height: 100rpx;
    vertical-align: middle;
}

.bbmenu .bd {
    margin-left: 20rpx;
    margin-top: 20rpx;
    vertical-align: middle;
}

.bbmenu .bdbtn {
    text-align: center;
    line-height: 18rpx;
    width: 120rpx;
    float: left;
    margin-top: 5rpx;
}

.bbmenu .bdbtn2 {
    height: 70rpx;
    width: 80%;
    float: right;
    margin-right: 20rpx;
    background: #00AE67;
    color: #fff;
    text-align: center;
    vertical-align: middle;
    font-size: 35rpx;
    display: table-cell;
    line-height: 70rpx;
    border-radius: 5rpx;
}