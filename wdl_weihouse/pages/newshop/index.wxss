.container {
    color: #333;
    font-family: -apple-system-font,"Helvetica Neue",sans-serif;
    padding-bottom: 80rpx;
}

.section {
    padding-left: 0rpx;
    background-color: #fff;
    padding-bottom: 20rpx;
    margin-bottom: 15rpx;
    padding-top: 20rpx;
}

.bage-blue1 {
    padding-right: 10rpx;
}

.mb0 {
    margin-bottom: 0;
    padding-bottom: 20rpx;
}

.faceimg {
    height: 460rpx;
}

.faceimg image {
    height: 460rpx;
}

.real {
    position: relative;
}

.addfav {
    position: absolute;
    top: 0;
    right: 0rpx;
    padding: 0 10rpx;
}

.addfav .iconfont {
    font-size: 45rpx;
    color: #fff;
}

.viewpic {
    position: absolute;
    bottom: 0;
    left: 50%;
    background: #000;
    opacity: .5;
    color: #fff;
    padding: 0 20rpx;
    border-radius: 10rpx;
    margin-left: -40rpx;
}

.sharepaper {
    position: absolute;
    bottom: 0;
    right: 0rpx;
    background: #000;
    opacity: .5;
    color: #fff;
    padding: 0 20rpx;
    border-radius: 10rpx;
    height: 60rpx;
    line-height: 60rpx;
}

.badge {
    vertical-align: middle;
    padding: 0 5rpx;
}

.badge2 {
    background: #FDF1DC;
    color: #EEBA6C;
    vertical-align: middle;
    padding: 0 5rpx;
}

.badge3 {
    background: #D7F2FB;
    color: #67A4D9;
    vertical-align: middle;
    padding: 0 5rpx;
}

.newshop .share {
    float: right;
    display: table-column;
    text-align: center;
}

.newshop .share button::after {
    background: none;
    border: none;
}

.newshop .share view {
    height: 35rpx;
    line-height: 35rpx;
    color: #16C98C;
}

.click {
    font-size: 28rpx;
    color: #999;
}

.click text {
    margin-right: 20rpx;
}

.pdleft {
    padding-left: 30rpx;
}

.th1 {
    padding-top: 0rpx;
}

.th1 .newshop {
    height: 70rpx;
    line-height: 70rpx;
}

.th1 .newshop text {
    margin-right: 10rpx;
}

.th1 .newshop .lptitle {
    font-weight: bold;
    color: #333;
}

.alex-label .bd {
    border: 1rpx solid #dadada;
    padding: 0rpx 10rpx;
    border-radius: 5rpx;
    background-color: #fff;
    font-size: 24rpx;
    margin-right: 10rpx;
    color: #999;
}

.alex-label .bd2 {
    padding: 10rpx 10rpx;
    border-radius: 5rpx;
    background-color: #E2F9E4;
    font-size: 24rpx;
    margin-right: 10rpx;
    color: #9DB89E;
}

.alex-label .bd2:nth-child(odd) {
    background-color: #EDF1FD;
    color: #9DB89E;
}

.th1 .lpaddress {
    margin-top: 10rpx;
    overflow: hidden;
}

.th1 .lpitem {
    white-space: nowrap;
    text-overflow: ellipsis;
    font-size: 35rpx;
    height: 60rpx;
    line-height: 60rpx;
}

.th1 .lpitem .time {
    color: #999;
    margin-left: 20rpx;
}

.th1 .lpitem .num {
    color: #cc3322;
    font-weight: bold;
}

.th1 .lpitem .more {
    float: right;
    margin-right: 30rpx;
    color: #666;
    font-size: 35rpx;
}

.th1 .lpitem .txt {
    float: left;
    font-size: 30rpx;
}

.th1 .lpitem .link {
    color: #67A4D9;
    width: 500rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    display: block;
    font-size: 30rpx;
}

.th1 .youhui {
    background-color: #FFEAED;
    border-left: 2px dashed #fff;
    border-right: 2px dashed #fff;
    margin-right: 30rpx;
    height: 100rpx;
    vertical-align: middle;
    margin-top: 30rpx;
}

.th1 .youhui .title {
    float: left;
    width: 75%;
    color: #cc3322;
    height: 100rpx;
    line-height: 100rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.th1 .youhui .iconfont {
    margin-left: 20rpx;
    margin-right: 20rpx;
    color: #cc3322;
}

.th1 .youhui .btn {
    background-color: #cc3322;
    border-radius: 30rpx;
    font-size: 30rpx;
    color: #fff;
    margin-top: 30rpx;
    margin-right: 20rpx;
    padding: 0 15rpx;
}

.th1 .youhui .more {
    width: 20%;
    float: right;
    text-align: center;
    vertical-align: middle;
}

.bar-more {
    height: 60rpx;
    line-height: 60rpx;
    border-left: 10rpx solid #3176DE;
    padding-left: 20rpx;
    margin-bottom: 20rpx;
}

.bar-more .bar {
    font-weight: 400;
    color: #000;
    font-size: 35rpx;
}

.bar-more .more {
    float: right;
    color: #999;
    margin-right: 30rpx;
    margin-top: 0rpx;
}

.timeline {
    border-left: 1px solid #dadada;
    padding-left: 30rpx;
    margin-right: 30rpx;
}

.timeline .line {
    margin-bottom: 30rpx;
}

.timeline .line .time {
    position: relative;
    line-height: 70rpx;
    margin-top: 0rpx;
    vertical-align: top;
    height: 70rpx;
}

.timeline .line .time .txt {
    font-size: 34rpx;
}

.timeline .tags {
    height: 90rpx;
    line-height: 90rpx;
}

.timeline .tags text {
    border: 1px solid #67A4D9;
    border-radius: 10rpx;
    background-color: #D7F2FB;
    padding: 2rpx 10rpx;
    color: #67A4D9;
}

.timeline .line .dot {
    width: 20rpx;
    height: 20rpx;
    border-radius: 50%;
    background: #999;
    vertical-align: middle;
    text-align: center;
    border: 5rpx solid #fff;
    display: inline-block;
    position: absolute;
    left: -42rpx;
    top: 20rpx;
}

.timeline .line-title {
    line-height: 40rpx;
}

.timeline .line-detail {
    line-height: 40rpx;
    margin-top: 15rpx;
    color: #666;
}

.twoline {
    word-break: break-all;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}

.baseinfo .bline .lbtxt {
    color: #999;
}

.realhouse {
    white-space: nowrap;
    display: flex;
    background-color: #ffffff;
}

.fiximg {
    width: 330rpx;
    height: 400rpx;
    display: inline-block;
    margin-left: 30rpx;
}

.fiximg image {
    width: 330rpx;
    height: 240rpx;
}

.fiximg .title text {
    margin-right: 10rpx;
}

.fiximg .area text {
    color: #999;
    margin-right: 20rpx;
}

.fiximg .price .fbr {
    color: #cc3322;
    font-weight: 400;
}

.fiximg .title,.fiximg .area,.fiximg .price {
    line-height: 34rpx;
    width: 90%;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.roombd {
    background: #f3f3f3;
    margin-top: -10px;
    height: 320rpx;
}

.room {
    white-space: nowrap;
    display: flex;
    height: 300rpx;
}

.roomimg {
    width: 380rpx;
    display: inline-block;
    background-color: #ffffff;
    margin-right: 20rpx;
    padding: 30rpx;
    border-radius: 5rpx;
    box-shadow: 1px 1px 2px #ddd;
}

.roomimg .title text {
    margin-right: 10rpx;
}

.roomimg .area text {
    color: #999;
    margin-right: 20rpx;
}

.roomimg .price .fbr {
    color: #cc3322;
    font-weight: 400;
}

.roomimg .title,.roomimg .area,.roomimg .price {
    line-height: 34rpx;
}

.housepicbd {
    white-space: nowrap;
    display: flex;
    height: 240rpx;
    padding-right: 10rpx;
    width: 100%;
}

.hxtu {
    white-space: nowrap;
    display: flex;
    height: 280rpx;
    padding-right: 10rpx;
    width: 100%;
}

.housepic-item {
    display: inline-block;
    width: 240rpx;
    text-align: center;
}

.housepic-image {
    width: 220rpx;
    height: 220rpx;
}

.housepic-name {
    line-height: 30rpx;
    width: 100%;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.copy {
    background-color: #eee;
    padding: 30rpx 60rpx;
    position: relative;
}

.copy .info {
    color: #666666;
    margin-left: 20rpx;
}

.copy text {
    display: table-cell;
}

.copy .copyicon {
    position: absolute;
    left: 30rpx;
    top: 25rpx;
    display: inline-block;
}

.copy .info {
    line-height: 30rpx;
}

.copy .info .ft22 {
    font-size: 22rpx;
    line-height: 30rpx;
    text-justify: inter-word;
    text-align: justify;
}

.bbmenu {
    position: fixed;
    bottom: 0px;
    left: 0px;
    width: 100%;
    border-top: #fff 1px solid;
    background: #fff;
    height: 95rpx;
    display: block;
}

.bbmenu .item {
    width: 22%;
    height: 95rpx;
    color: #222;
    display: inline-block;
}

.bbmenu .item2 {
    flex-grow: 2;
    width: 33.3%;
    height: 95rpx;
    background: #1296DB;
    color: #fff;
    display: inline-block;
}

.bbmenu .favbtn {
    color: #fff;
    text-align: center;
    height: 100%;
    font-size: 32rpx;
    line-height: 95rpx;
}

.bbmenu .item .iconfont {
    color: #1296DB;
}

.bbmenu .bdbtn {
    text-align: center;
    line-height: 35rpx;
    height: 100%;
    margin-top: 10rpx;
}

.bbmenu .bdbtn2 {
    padding-top: 15rpx;
    height: 95rpx;
    background: #cc3322;
    color: #fff;
    text-align: center;
    font-size: 32rpx;
    line-height: 35rpx;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}